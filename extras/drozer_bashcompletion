    _drozer()
    {
        local cur prev opts
        COMPREPLY=()
        cur="${COMP_WORDS[COMP_CWORD]}"
        prev="${COMP_WORDS[COMP_CWORD-1]}"

        opts=`/path/to/drozer-complete ${COMP_WORDS[@]} $COMP_CWORD 2> /dev/null`

        COMPREPLY=( $(compgen -W "${opts}" -- ${cur}) )
        return 0
    }
    complete -F _drozer drozer